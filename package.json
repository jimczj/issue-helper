{
  "name": "issue-helper",
  "version": "0.1.7",
  "description": "Generate issue helper static pages through configuration",
  "bin": {
    "issue-helper": "bin/issue.js"
  },
  "files": [
    "bin",
    "dist",
    "config.js"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js --open",
    "build": "webpack --config webpack.config.js --hide-modules",
    "deploy": "npm run build && http-server ./dist",
    "lint": "eslint ./src --fix",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix"
  },
  "lint-staged": {
    "src/**/*.(js|jsx)": [
      "eslint",
      "git add"
    ],
    "src/**/*.(ts|tsx)": [
      "tslint",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/node": "7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.49",
    "@babel/plugin-transform-react-jsx": "7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/runtime": "7.0.0-beta.49",
    "babel-eslint": "8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14",
    "husky": "^1.0.0-rc.13",
    "json-loader": "0.5.7",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.11",
    "markdown": "^0.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.6",
    "prettier": "1.13.4",
    "raw-loader": "0.5.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "antd": "^3.12.4",
    "autoprefixer": "^9.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "compare-versions": "^3.4.0",
    "cross-env": "5.1.6",
    "css-loader": "^1.0.0",
    "es5-polyfill": "0.0.6",
    "es6-object-assign": "1.1.0",
    "es6-promise": "4.2.4",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^4.0.0-alpha",
    "eslint": "4.19.1",
    "eslint-config-o2team": "^0.1.6",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-standard": "3.1.0",
    "stylelint": "9.2.1",
    "stylelint-config-prettier": "3.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "^3.4.0",
    "ts-loader": "^4.4.2",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-config-standard": "7.0.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "typescript": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jimczj/taro-ui-issue-helper.git"
  },
  "bugs": {
    "url": "https://github.com/jimczj/taro-ui-issue-helper/issues"
